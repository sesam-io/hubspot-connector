[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-children",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-lineitem-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "if",
              [
                "is-empty",
                "_S.associations"
              ],
              [
                "filter"
              ]
            ],
            [
              "copy",
              "id"
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "{{@ datatype @}}_list",
        "replace_entity": false,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "create-child",
              [
                "apply",
                "rows",
                "_S.response.results"
              ]
            ]
          ],
          "rows": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "id",
              "_P._S.id"
            ],
            [
              "add",
              "_id",
              [
                "concat",
                "_T.id",
                "-",
                [
                  "string",
                  "_R._S.response.results.typeId"
                ]
              ]
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-children",
      "type": "dataset"
    },
    "transform": [
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  }
]