{
  "_id": "{{@ system @}}-{{@ datatype @}}-collect",
  "namespaced_identifiers": false,
  "source": {
    "id_expression": "{{ id }}",
    "operation": "alternate_list",
    "payload_property": "results",
    "properties": {
      "datatype": "crm/v3/{{@ datatype @}}s"
    },
    "system": "{{@ system @}}",
    "type": "rest"
  },
  "transform": {
    "rules": {
      "default": [
        [
          "copy",
          "*"
        ],
        [
          "add",
          "updatedAt",
          [
            "datetime-parse",
            "%Y-%m-%dT%H:%M:%S.%fZ",
            "_S.updatedAt"
          ]
        ]
      ]
    },
    "type": "dtl"
  },
  "type": "pipe"
}