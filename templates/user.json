[
  {
    "_id": "xxxxxxxx-hubspot-user-collect",
    "type": "pipe",
    "source": {
      "type": "rest",
      "system": "xxxxxxxx-hubspot",
      "id_expression": "{{ id }}",
      "operation": "get-settings",
      "payload_property": "results",
      "properties": {
        "url": "users/"
      },
      "updated_expression": "{{ updatedAt }}"
    },
    "add_namespaces": false
  },
  {
    "_id": "xxxxxxxx-hubspot-user-share-update",
    "type": "pipe",
    "source": {
      "type": "dataset",
      "dataset": "xxxxxxxx-hubspot-user-transform"
    },
    "sink": {
      "type": "rest",
      "system": "xxxxxxxx-hubspot",
      "operation": "update-settings"
    },
    "transform": [
      {
        "type": "dtl",
        "rules": {
          "default": [
            [
              "discard",
              [
                "is-not-empty",
                "_S.id"
              ]
            ],
            [
              "add",
              "::properties",
              [
                "dict",
                "url",
                [
                  "concat",
                  "users/",
                  "_S.id"
                ]
              ]
            ],
            [
              "add",
              "::entity",
              "_S."
            ]
          ]
        }
      },
      {
        "type": "rest",
        "system": "xxxxxxxx-hubspot",
        "operation": "get-settings",
        "replace_entity": false
      },
      {
        "type": "dtl",
        "rules": {
          "default": [
            [
              "comment",
              "**** OPTIMISTIC LOCKING ****"
            ],
            [
              "add",
              "_old",
              [
                "first",
                [
                  "hops",
                  {
                    "datasets": [
                      "xxxxxxxx-hubspot-user-collect a"
                    ],
                    "where": [
                      [
                        "eq",
                        "_S.entity.id",
                        "a.id"
                      ]
                    ]
                  }
                ]
              ]
            ],
            [
              "add",
              "_json_old",
              [
                "json-transit",
                [
                  "apply",
                  "remove-under",
                  "_T._old"
                ]
              ]
            ],
            [
              "add",
              "_json_new",
              [
                "first",
                [
                  "json-transit",
                  [
                    "apply",
                    "remove-under",
                    [
                      "first",
                      "_S.response"
                    ]
                  ]
                ]
              ]
            ],
            [
              "add",
              "_hash_old",
              [
                "hash128",
                "murmur3",
                "_T._json_old"
              ]
            ],
            [
              "add",
              "_hash_new",
              [
                "hash128",
                "murmur3",
                "_T._json_new"
              ]
            ],
            [
              "if",
              [
                "eq",
                "_T._hash_old",
                "_T._hash_new"
              ],
              [
                [
                  "comment",
                  "**** SAME DATA IN SYSTEM AS IN SESAM ****"
                ],
                [
                  "add",
                  "::payload",
                  [
                    "apply",
                    "remove-under",
                    "_S.entity"
                  ]
                ],
                [
                  "add",
                  "::properties",
                  [
                    "dict",
                    "url",
                    [
                      "concat",
                      "users/",
                      "_S.entity.id"
                    ]
                  ]
                ]
              ],
              [
                [
                  "comment",
                  "**** DIFFERENT DATA IN SYSTEM THAN IN SESAM ****"
                ],
                [
                  "discard"
                ]
              ]
            ]
          ],
          "remove-under": [
            [
              "copy",
              "*",
              "_*"
            ]
          ]
        }
      }
    ],
    "batch_size": 1
  }
]