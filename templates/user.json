[
  {
    "_id": "{{@ system @}}-user-collect",
    "type": "pipe",
    "source": {
      "type": "rest",
      "system": "{{@ system @}}",
      "id_expression": "{{ id }}",
      "operation": "get-settings",
      "payload_property": "results",
      "properties": {
        "url": "users/"
      },
      "updated_expression": "{{ updatedAt }}"
    },
    "add_namespaces": false
  },
  {
    "_id": "{{@ system @}}-hubspot-user-share-insert",
    "type": "pipe",
    "source": {
      "type": "dataset",
      "dataset": "{{@ system @}}-hubspot-user-transform",
      "subset": ["eq",
        ["is-empty", "_S.id"], true]
    },
    "sink": {
      "set_initial_offset": "onload"
    },
    "transform": [{
      "type": "dtl",
      "rules": {
        "default": [
          ["discard",
            ["is-null", "_S.id"]
          ],
          ["discard",
            ["is-empty",
              ["hops", {
                "datasets": ["{{@ system @}}-hubspot-user-share-insert chi"],
                "where": [
                  ["eq", "_S.sesam-id", "chi.sesam-id"]
                ]
              }]
            ]
          ],
          ["add", "::payload",
            ["dict", "email", "_S.email", "sendWelcomeEmail", true]
          ],
          ["copy", "sesam-id"]
        ]
      }
    }, {
      "type": "rest",
      "system": "{{@ system @}}-hubspot",
      "operation": "insert-settings",
      "properties": {
        "url": "users"
      },
      "replace_entity": false
    }, {
      "type": "dtl",
      "rules": {
        "default": [
          ["copy", "sesam-id"],
          ["merge-union", "_S.response"],
          ["add", "_id", "_T.id"]
        ]
      }
    }],
    "pump": {
      "run_at_startup_if_not_populated": true
    },
    "add_namespaces": false,
    "disable_set_last_seen": true,
    "reprocessing_policy": "continue"
  },
  {
    "_id": "{{@ system @}}-user-share-update",
    "type": "pipe",
    "source": {
      "type": "dataset",
      "dataset": "{{@ system @}}-user-transform"
    },
    "sink": {
      "type": "rest",
      "system": "{{@ system @}}",
      "operation": "update-settings"
    },
    "transform": [
      {
        "type": "dtl",
        "rules": {
          "default": [
            [
              "discard",
              [
                "is-not-empty",
                "_S.id"
              ]
            ],
            [
              "add",
              "::properties",
              [
                "dict",
                "url",
                [
                  "concat",
                  "users/",
                  "_S.id"
                ]
              ]
            ],
            [
              "add",
              "::entity",
              "_S."
            ]
          ]
        }
      },
      {
        "type": "rest",
        "system": "{{@ system @}}",
        "operation": "get-settings",
        "replace_entity": false
      },
      {
        "type": "dtl",
        "rules": {
          "default": [
            [
              "comment",
              "**** OPTIMISTIC LOCKING ****"
            ],
            [
              "add",
              "_old",
              [
                "first",
                [
                  "hops",
                  {
                    "datasets": [
                      "{{@ system @}}-user-collect a"
                    ],
                    "where": [
                      [
                        "eq",
                        "_S.entity.id",
                        "a.id"
                      ]
                    ]
                  }
                ]
              ]
            ],
            [
              "add",
              "_json_old",
              [
                "json-transit",
                [
                  "apply",
                  "remove-under",
                  "_T._old"
                ]
              ]
            ],
            [
              "add",
              "_json_new",
              [
                "first",
                [
                  "json-transit",
                  [
                    "apply",
                    "remove-under",
                    [
                      "first",
                      "_S.response"
                    ]
                  ]
                ]
              ]
            ],
            [
              "add",
              "_hash_old",
              [
                "hash128",
                "murmur3",
                "_T._json_old"
              ]
            ],
            [
              "add",
              "_hash_new",
              [
                "hash128",
                "murmur3",
                "_T._json_new"
              ]
            ],
            [
              "if",
              [
                "eq",
                "_T._hash_old",
                "_T._hash_new"
              ],
              [
                [
                  "comment",
                  "**** SAME DATA IN SYSTEM AS IN SESAM ****"
                ],
                [
                  "add",
                  "::payload",
                  [
                    "apply",
                    "remove-under",
                    "_S.entity"
                  ]
                ],
                [
                  "add",
                  "::properties",
                  [
                    "dict",
                    "url",
                    [
                      "concat",
                      "users/",
                      "_S.entity.id"
                    ]
                  ]
                ]
              ],
              [
                [
                  "comment",
                  "**** DIFFERENT DATA IN SYSTEM THAN IN SESAM ****"
                ],
                [
                  "discard"
                ]
              ]
            ]
          ],
          "remove-under": [
            [
              "copy",
              "*",
              "_*"
            ]
          ]
        }
      }
    ],
    "batch_size": 1
  }
]