                    "track-dependencies": false
                  }
                ]
              ]
            ],
            [
              "add",
              "::payload",
              [
                "dict",
                "properties",
                "_S.properties"
              ]
            ],
            [
              "copy",
              "sesam-id"
            ]
          ]
        }
      },
      {
        "type": "rest",
        "system": "{{@ system @}}",
        "operation": "insert",
        "properties": {
          "url": "contacts"
        },
        "replace_entity": false,
        "side_effects": true
      },
      {
        "type": "dtl",
        "rules": {
          "default": [
            [
              "copy",
              "sesam-id"
            ],
            [
              "merge-union",
              "_S.response"
            ],
            [
              "add",
              "_id",
              "_T.id"
            ]
          ]
        }
      }
    ],
    "pump": {
      "run_at_startup_if_not_populated": true
    },
    "add_namespaces": false,
    "disable_set_last_seen": true,
    "reprocessing_policy": "continue"
  },
